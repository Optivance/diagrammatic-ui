{
  "name": "react-graph-viz",
  "version": "0.1.0",
  "description": "A flexible, customizable graph visualization library for React",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:types": "tsc --emitDeclarationOnly",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "demo": "vite --config demo/vite.config.ts",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "leader-line": "^1.0.8",
    "react-feather": "^2.0.10",
    "lucide-react": "^0.483.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "tailwindcss": "^4.0.16",
    "typescript": "~5.7.2",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^1.2.0"
  },
  "keywords": [
    "react",
    "graph",
    "visualization",
    "network",
    "diagram",
    "nodes",
    "edges",
    "typescript"
  ],
  "author": "",
  "license": "MIT"
}
